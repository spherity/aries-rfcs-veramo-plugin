    "@veramo/cli": "^5.1.2",
    "cross-env": "^7.0.3",
    "jest": "29.4.3",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "files": [
    "build/**/*",
    "src/**/*",
    "README.md",
    "plugin.schema.json",
    "LICENSE"
  ],
  "dependencies": {
    "@veramo/core-types": "^5.1.2",
    "@veramo/did-manager": "^5.1.2",
    "@veramo/key-manager": "^5.1.2"
