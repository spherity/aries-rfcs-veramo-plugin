{
  "name": "veramo-plugin-template",
  "version": "1.0.0",
  "source": "src/index.ts",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "veramo": {
    "pluginInterfaces": {
      "IMyAgentPlugin": "./src/types/IMyAgentPlugin.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate-plugin-schema": "yarn veramo dev generate-plugin-schema",
